{
    "install_sbom_latest": {
        "image": "mcr.microsoft.com/devcontainers/base:ubuntu",
        "features": {
            "sbom": {
                "version": "latest"
            }
        }
    },
    "install_sbom_specific_version": {
        "image": "mcr.microsoft.com/devcontainers/base:ubuntu",
        "features": {
            "sbom": {
                "version": "3.0.1"
            }
        }
    },
    "install_sbom_custom_alias": {
        "image": "mcr.microsoft.com/devcontainers/base:ubuntu",
        "features": {
            "sbom": {
                "version": "latest",
                "alias": "msft-sbom"
            }
        }
    },
    "invalid_version": {
        "image": "mcr.microsoft.com/devcontainers/base:ubuntu",
        "features": {
            "sbom": {
                "version": "latest"
            }
        }
    }
}
