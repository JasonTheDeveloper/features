{
    "name": "Microsoft SBOM-Tool",
    "id": "sbom",
    "version": "1.1.1",
    "description": "A feature to install Microsoft SBOM-Tool to help generate SBOMs",
    "options": {
        "version": {
            "type": "string",
            "proposals": [
                "latest",
                "0.3.3"
            ],
            "default": "latest",
            "description": "Select or enter sbom-tool version"
        },
        "alias": {
            "type": "string",
            "proposals": [
                "sbom-tool",
                "msft-sbom",
                "sbom"
            ],
            "default": "sbom-tool",
            "description": "Select or enter an alias used to run the sbom-tool"
        }
    }
}
